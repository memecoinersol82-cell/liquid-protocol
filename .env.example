# Helius RPC URL
HELIUS_RPC_URL=https://mainnet.helius-rpc.com/?api-key=YOUR_API_KEY

# Private key (base58 encoded)
PRIVATE_KEY=your_base58_private_key_here

# Token mint address (Token-2022)
TOKEN_MINT=your_token_mint_address_here

# Minimum fee threshold in SOL to trigger claim (default: 0.015)
MIN_FEE_THRESHOLD=0.015

# Buyback percentage (default: 50 = 50%)
BUYBACK_PERCENTAGE=50

# Bot check interval in milliseconds (default: 60000 = 1 minute)
CHECK_INTERVAL=60000

# Server port
PORT=3000
